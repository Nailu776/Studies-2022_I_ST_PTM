<?xml version="1.0" encoding="utf-8"?>
<!-- Prostokąt - o równych wymiarach == Kwadrat -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--    Rozmiar 25x25 [dp]-->
    <size
        android:width="25dp"
        android:height="25dp"/>
    <!--    Kolor wypełnienia-->
    <solid
        android:color="@color/blue_sq"/>
    <!--    Kolor krawędzi-->
    <stroke
        android:color="@color/dark_blue_sq"
        android:width="1dp"/>
    <!--    Zaokrąglone rogi-->
    <corners
        android:radius="5dp"/>
</shape>